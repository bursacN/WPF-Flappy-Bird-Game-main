<Window x:Class="Flappy_Bird_WPF_MOO_ICT.EndScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Flappy_Bird_WPF_MOO_ICT"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen"
        WindowStyle="SingleBorderWindow"
        Title="Flappy Bird" Height="200" Width="400"
        Background="LightBlue"
        PreviewKeyDown="Window_PreviewKeyDown">
    <Grid>
        <!-- Your existing XAML content here -->
        <Button Content="Back To Menu" HorizontalAlignment="Left" Margin="10,132,0,0" VerticalAlignment="Top" Click="Button_Click" Width="110" Height="25"  FontWeight="Bold" FontSize="14" RenderTransformOrigin="0.353,0.482"/>
        <Button Content="Scores" HorizontalAlignment="Left" Margin="263,132,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.279,-0.105"  Click="Button_Click_1" Width="110" Height="25"  FontWeight="Bold" FontSize="14"/>
        <Label Name="MsgTop" Content="Game ended " HorizontalAlignment="Center" Margin="0,13,0,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="14"/>
        <Label Content="Press R to start playing" HorizontalAlignment="Center" VerticalAlignment="Top" RenderTransformOrigin="0.513,2.374" Margin="0,86,0,0" FontWeight="Bold" FontSize="14"/>
        <Label Name="YourScore" Content="Your score: " HorizontalAlignment="Left" Margin="282,39,0,0" VerticalAlignment="Top" FontWeight="Bold" />
        <Label Name="HighestScore" Content="Highest score: " HorizontalAlignment="Left" Margin="264,65,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.476,0.551" FontWeight="Bold"/>
    </Grid>
</Window>
